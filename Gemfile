source 'https://rubygems.org'
gem 'github-pages', group: :jekyll_plugins


group :jekyll_plugins do
    gem 'jekyll', '3.6.2'
    gem "jekyll-feed", "~> 0.6"
    gem "jekyll-youtube"
    gem 'jekyll-sitemap'
    gem 'jekyll-twitter-plugin'
    gem 'jekyll-paginate-v2'
    gem 'jekyll-seo-tag'

    gem 'jekyll-tagging', :git => 'https://github.com/pattex/jekyll-tagging.git'
 end

